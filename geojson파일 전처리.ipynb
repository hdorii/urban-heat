   "source": [
    "import geopandas as gpd\n",
    "\n",
    "gdf = gpd.read_file(\"data/seoul_districts.geojson\")\n",
    "\n",
    "# 'sggnm' (자치구명) 기준으로 병합\n",
    "merged = gdf.dissolve(by=\"sggnm\", as_index=False)\n",
    "\n",
    "# 저장\n",
    "merged.to_file(\"data/seoul_gu_25.geojson\", driver=\"GeoJSON\")"
   ]
  }
 ],
